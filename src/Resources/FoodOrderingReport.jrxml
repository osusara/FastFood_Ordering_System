	<queryString>
		<![CDATA[SELECT
     restaurentsystem.order.`order_id` AS order_order_id,
     restaurentsystem.order.`date` AS order_date,
     restaurentsystem.order.`total` AS order_total,
     restaurentsystem.order.`recieved` AS order_recieved,
     restaurentsystem.order.`balance` AS order_balance,
     restaurentsystem.order.`customer_id` AS order_customer_id,
     restaurentsystem.order.`user_id` AS order_user_id,
     order_has_dessert.`order_id` AS order_has_dessert_order_id,
     order_has_dessert.`dessert_id` AS order_has_dessert_dessert_id,
     order_has_dessert.`qty` AS order_has_dessert_qty,
     order_has_drink.`order_id` AS order_has_drink_order_id,
     order_has_drink.`drink_id` AS order_has_drink_drink_id,
     order_has_drink.`qty` AS order_has_drink_qty,
     order_has_meal.`order_id` AS order_has_meal_order_id,
     order_has_meal.`meal_id` AS order_has_meal_meal_id,
     order_has_meal.`qty` AS order_has_meal_qty,
     customer.`customer_id` AS customer_customer_id,
     customer.`name` AS customer_name,
     customer.`phone` AS customer_phone,
     customer.`email` AS customer_email,
     meal.`meal_id` AS meal_meal_id,
     meal.`name` AS meal_name,
     meal.`unit_price` AS meal_unit_price,
     drink.`drink_id` AS drink_drink_id,
     drink.`name` AS drink_name,
     drink.`unit_price` AS drink_unit_price,
     dessert.`dessert_id` AS dessert_dessert_id,
     dessert.`name` AS dessert_name,
     dessert.`unit_price` AS dessert_unit_price

FROM restaurentsystem.order
     INNER JOIN `order_has_dessert` order_has_dessert ON restaurentsystem.order.`order_id` = order_has_dessert.`order_id`
     INNER JOIN `order_has_drink` order_has_drink ON restaurentsystem.order.`order_id` = order_has_drink.`order_id`
     INNER JOIN `order_has_meal` order_has_meal ON restaurentsystem.order.`order_id` = order_has_meal.`order_id`
     INNER JOIN `customer` customer ON restaurentsystem.order.`customer_id` = customer.`customer_id`
     INNER JOIN `meal` meal ON order_has_meal.`meal_id` = meal.`meal_id`
     INNER JOIN `drink` drink ON order_has_drink.`drink_id` = drink.`drink_id`
     INNER JOIN `dessert` dessert ON order_has_dessert.`dessert_id` = dessert.`dessert_id`]]>
	</queryString>
